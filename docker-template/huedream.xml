<?xml version="1.0"?>
<Container version="2">
  <Date>2019-12-20</Date>
  <Beta>False</Beta>
  <Category>Tools</Category>
  <Name>HueDream</Name>
  <Project>https://github.com/d8ahazard/HueDream</Project>
  <Overview>
    HueDream synchronizes your Hue Lights with a DreamScreen controller.
  </Overview>
  <Description>
   HueDream synchronizes your Hue Lights with a DreamScreen controller.
  </Description>
  <Registry>https://hub.docker.com/r/digitalhigh/huedream</Registry>
  <GitHub>https://github.com/d8ahazard/unraid-repository</GitHub>
  <Repository>digitalhigh/huedream</Repository>
  <Support>https://github.com/d8ahazard/huedream</Support>
  <BindTime>true</BindTime>
  <Registry/>
  <Network>host</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/huedream/</HostDir>
      <ContainerDir>/etc/huedream</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>      
    </Publish>
  </Networking>  
  <DonateText>If you like it, consider buying me a beer.</DonateText>
  <DonateLink>https://www.paypal.me/donate.to.digitalhigh</DonateLink>
  <DonateImg>https://www.paypal.com/en_US/i/btn/btn_donate_SM.gif</DonateImg>
  <WebUI>http://[IP]:[PORT:5666]</WebUI>
  <Banner></Banner>
  <Icon>https://github.com/d8ahazard/HueDream/blob/master/wwwroot/img/ic_launcher.png?raw=true</Icon>
<Labels/>
  <Config Name="UserData Location" Target="/etc/huedream" Default="" Mode="rw" Description="Where to store HueData config." Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/huedream/</Config>
  <Config Type="Port" Name="Hue Discovery" Target="1900" Default="1900" Mode="udp" Description="Hue Bridge discovery port." Display="always" Required="true">1900</Config>
  <Config Type="Port" Name="Hue MDNS Discovery" Target="5353" Default="5353" Mode="udp" Description="Hue Bridge discovery port (MDNS)." Display="always" Required="true">5353</Config>
  <Config Type="Port" Name="Hue Streaming" Target="2100" Default="2100" Mode="udp" Description="Hue Bridge streaming port." Display="always" Required="true">2100</Config>
  <Config Type="Port" Name="Web UI" Target="5699" Default="5699" Mode="udp" Description="Web UI port." Display="always" Required="true">5699</Config>
  <Config Type="Port" Name="DS Communication" Target="8888" Default="8888" Mode="udp" Description="DreamScreen emulation port." Display="always" Required="true">8888</Config>
</Container>